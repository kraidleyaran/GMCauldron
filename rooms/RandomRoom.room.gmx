  <code>randomize()
var totalRoomCount = 10;
var roomCount = 0;


var unusedRoomList = CreateNewList();
var usedRoomList = CreateNewList();
var unbuiltRoomsList = CreateNewList();

for (p = 0; p &lt; 5; p += 1)
{
    for (q = 0; q &lt; 5; q += 1)
    {
        AddToList(unusedRoomList, string(p) + string(q));        
    }
}
ShuffleList(unusedRoomList);

var firstRoom = GetFromList(unusedRoomList,0);
AddToList(usedRoomList, firstRoom);
AddToList(unbuiltRoomsList, firstRoom);
RemoveFromList(unusedRoomList, 0);
roomCount += 1;

for (i = 1; i &lt; totalRoomCount; i += 1)
{
    ShuffleList(usedRoomList);
    var getRoom = GetFromList(usedRoomList, 0);
    i += GetAvailableRoom(unusedRoomList, usedRoomList,unbuiltRoomsList,getRoom,10, 10, totalRoomCount - i)
}
for (p = 0; p &lt; totalRoomCount; p += 1)
{
    var buildRoom = GetFromList(usedRoomList, p);
    
    var roomX = real(string_char_at(buildRoom, 0));
    var roomY = real(string_char_at(buildRoom, 1));
    
    if !(roomX == 0)
    {
        roomX = 240 * roomX;
    }
    if !(roomY == 0)
    {
        roomY = 160 * roomY;
    }

    instance_create(roomX,roomY, DungeonRoom);
}</code>
